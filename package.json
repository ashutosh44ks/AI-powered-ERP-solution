{
  "name": "ai-dashboard",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start:client": "npm --prefix client run dev",
    "start:server": "npm --prefix server run dev",
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "dev": "npm run start"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
